    "liveNow": false,
    "panels": [
        {
            "title": "üöÄ AI-RPC Overview",
            "type": "row",
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 0
            }
        },
        {
            "title": "Request Rate",
            "type": "stat",
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "sum(rate(rpc_server_requests_total[1m]))",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps",
                    "color": {
                        "mode": "palette-classic"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 100
                            },
                            {
                                "color": "red",
                                "value": 500
                            }
                        ]
                    }
                }
            },
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 0,
                "y": 1
            }
        },
        {
            "title": "Average Latency",
            "type": "stat",
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "avg(rate(rpc_server_processing_seconds_sum[1m]) / rate(rpc_server_processing_seconds_count[1m]))",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "color": {
                        "mode": "palette-classic"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 0.1
                            },
                            {
                                "color": "red",
                                "value": 0.5
                            }
                        ]
                    }
                }
            },
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 6,
                "y": 1
            }
        },
        {
            "title": "Error Rate",
            "type": "gauge",
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "sum(rate(rpc_server_requests_total{status=\"error\"}[1m])) / sum(rate(rpc_server_requests_total[1m])) * 100",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "min": 0,
                    "max": 100,
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 5
                            },
                            {
                                "color": "red",
                                "value": 20
                            }
                        ]
                    }
                }
            },
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 12,
                "y": 1
            }
        },
        {
            "title": "Active Providers",
            "type": "stat",
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "count(up{job=\"rpc_provider\"} == 1)",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "none",
                    "color": {
                        "mode": "palette-classic"
                    }
                }
            },
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 18,
                "y": 1
            }
        },
        {
            "title": "ü§ñ AI Load Balancer",
            "type": "row",
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 5
            }
        },
        {
            "title": "Health Scores by Instance",
            "type": "timeseries",
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "1 / (1 + (rate(rpc_server_processing_seconds_sum[1m]) / rate(rpc_server_processing_seconds_count[1m])) * 20)",
                    "legendFormat": "{{instance}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percentunit",
                    "min": 0,
                    "max": 1,
                    "custom": {
                        "lineWidth": 2,
                        "fillOpacity": 20
                    }
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 6
            }
        },
        {
            "title": "Latency per Instance",
            "type": "timeseries",
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "rate(rpc_server_processing_seconds_sum{job=\"rpc_provider\"}[1m]) / rate(rpc_server_processing_seconds_count{job=\"rpc_provider\"}[1m])",
                    "legendFormat": "{{instance}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "custom": {
                        "lineWidth": 2,
                        "fillOpacity": 10
                    }
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 6
            }
        },
        {
            "title": "üîí Circuit Breaker Status",
            "type": "row",
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 14
            }
        },
        {
            "title": "Circuit Breaker States",
            "type": "stat",
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "rpc_circuit_breaker_state",
                    "legendFormat": "{{service}} - {{state}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "mappings": [
                        {
                            "type": "value",
                            "options": {
                                "0": {
                                    "text": "CLOSED",
                                    "color": "green"
                                }
                            }
                        },
                        {
                            "type": "value",
                            "options": {
                                "1": {
                                    "text": "OPEN",
                                    "color": "red"
                                }
                            }
                        },
                        {
                            "type": "value",
                            "options": {
                                "2": {
                                    "text": "HALF_OPEN",
                                    "color": "yellow"
                                }
                            }
                        }
                    ]
                }
            },
            "gridPos": {
                "h": 4,
                "w": 8,
                "x": 0,
                "y": 15
            }
        },
        {
            "title": "Failure Rate per Service",
            "type": "timeseries",
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "rpc_circuit_breaker_failure_rate",
                    "legendFormat": "{{service}}",
                    "refId": "A"
