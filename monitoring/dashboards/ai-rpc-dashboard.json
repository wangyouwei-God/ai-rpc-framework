            "title": "P99 Latency Trend",
            "type": "timeseries",
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.99, sum(rate(rpc_server_processing_seconds_bucket[1m])) by (le, instance))",
                    "legendFormat": "P99 - {{instance}}",
                    "refId": "A"
                },
                {
                    "expr": "histogram_quantile(0.50, sum(rate(rpc_server_processing_seconds_bucket[1m])) by (le, instance))",
                    "legendFormat": "P50 - {{instance}}",
                    "refId": "B"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
